<!DOCTYPE html>
<html lang="en">
<%c++
    auto wellname=@@.get<std::string>("wellname");
    auto wellno=@@.get<std::string>("wellno");
    auto dateOfRecentTest=@@.get<std::string>("dateOfRecentTest");
    auto dailyOil=@@.get<std::string>("dailyOil");
    auto dailyWater=@@.get<std::string>("dailyWater");
    auto dailyGas=@@.get<std::string>("dailyGas");
    auto opPressureTubing=@@.get<std::string>("opPressureTubing");
    auto opPressureCasing=@@.get<std::string>("opPressureCasing");
    auto pumpingUnitSize=@@.get<std::string>("pumpingUnitSize");
    auto strokesPerMin=@@.get<std::string>("strokesPerMin");
    auto strokeLength=@@.get<std::string>("strokeLength");
    auto motorHp=@@.get<std::string>("motorHp");
    auto pumpingRatio=@@.get<std::string>("pumpingRatio");
    auto unitGearRatio=@@.get<std::string>("unitGearRatio");
    auto casingSize=@@.get<std::string>("casingSize");
    auto tubingSize=@@.get<std::string>("tubingSize");
    auto pumpSize=@@.get<std::string>("pumpSize");
    auto firstCole=@@.get<std::string>("firstCole");
    auto secondCole=@@.get<std::string>("secondCole");
    auto thirdCole=@@.get<std::string>("thirdCole");
    auto comments=@@.get<std::string>("comments");
%>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>UnitedGas 12</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
</head>
<body>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="wellname">Well Name:</label>
      <%c++ $$<<wellname; %>
      <input type="text" id="wellname" name="wellname">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="wellno">Well Number:</label>
      <%c++ $$<<wellno; %>
      <input type="text" id="wellno" name="wellno">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="dateOfRecentTest">Date of Recent Test:</label>
      <%c++ $$<<dateOfRecentTest; %>
      <input type="text" id="dateOfRecentTest" name="dateOfRecentTest">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="dailyOil">Daily Oil:</label>
      <%c++ $$<<dailyOil; %>
      <input type="text" id="dailyOil" name="dailyOil">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="dailyWater">Daily Water:</label>
      <%c++ $$<<dailyWater; %>
      <input type="text" id="dailyWater" name="dailyWater">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="dailyGas">Daily Gas:</label>
      <%c++ $$<<dailyGas; %>
      <input type="text" id="dailyGas" name="dailyGas">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="opPressureTubing">Operating Pressure Tubing:</label>
      <%c++ $$<<opPressureTubing; %>
      <input type="text" id="opPressureTubing" name="opPressureTubing">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="opPressureCasing">Operating Pressure Casing:</label>
      <%c++ $$<<opPressureCasing; %>
      <input type="text" id="opPressureCasing" name="opPressureCasing">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="pumpingUnitSize">Pumping Unit Size:</label>
      <%c++ $$<<pumpingUnitSize; %>
      <input type="text" id="pumpingUnitSize" name="pumpingUnitSize">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="strokesPerMin">Strokes Per Min:</label>
      <%c++ $$<<strokesPerMin; %>
      <input type="text" id="strokesPerMin" name="strokesPerMin">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="strokeLength">stroke Length:</label>
      <%c++ $$<<strokeLength; %>
      <input type="text" id="strokeLength" name="strokeLength">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="motorHp">Motor HP:</label>
      <%c++ $$<<motorHp; %>
      <input type="text" id="motorHp" name="motorHp">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="pumpingRatio">Pumping Ratio:</label>
      <%c++ $$<<pumpingRatio; %>
      <input type="text" id="pumpingRatio" name="pumpingRatio">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="unitGearRatio">Unit Gear Ratio:</label>
      <%c++ $$<<unitGearRatio; %>
      <input type="text" id="unitGearRatio" name="unitGearRatio">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="casingSize">Casing Size:</label>
      <%c++ $$<<casingSize; %>
      <input type="text" id="casingSize" name="casingSize">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="tubingSize">Tubing Size:</label>
      <%c++ $$<<tubingSize; %>
      <input type="text" id="tubingSize" name="tubingSize">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="pumpSize">Pump Size:</label>
      <%c++ $$<<pumpSize; %>
      <input type="text" id="pumpSize" name="pumpSize">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="firstCole">First Cole:</label>
      <%c++ $$<<firstCole; %>
      <input type="text" id="firstCole" name="firstCole">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="secondCole">Second Cole:</label>
      <%c++ $$<<secondCole; %>
      <input type="text" id="secondCole" name="secondCole">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="thirdCole">Third Cole:</label>
      <%c++ $$<<thirdCole; %>
      <input type="text" id="thirdCole" name="thirdCole">
  </form>
  <form action="/formHandler" method="post">
      <input type="submit" value="Update">
      <label for="comments">Comments:</label>
      <%c++ $$<<comments; %>
      <input type="text" id="comments" name="comments">
  </form>
  
<form action="/unitedgas/" method="get">
<div class="container">
</div>
<button type="submit">Home</button>
</form>

<form action="/logout" method="post">
<div class="container">
</div>
<button type="submit">Logout</button>
</form>

</body>
</html>
